{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["es2017"],
    "allowJs": true,
    "skipLibCheck": false,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-native"
  },
  "exclude": [
    "node_modules",
    ".babelrc.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
